Sub TurnCIon()

'works
'turns CI on

Dim sht As Worksheet
Dim CurrentSheet As Worksheet
Dim cht As ChartObject

Application.ScreenUpdating = False
Application.EnableEvents = False

Set CurrentSheet = ActiveSheet

CurrentSheet.Activate
'For Each sht In ActiveWorkbook.Worksheets


  
 For Each cht In ActiveSheet.ChartObjects
    cht.Activate
    
On Error Resume Next

   ActiveChart.FullSeriesCollection(4).Select
     
    Selection.MarkerStyle = -4115
     Selection.MarkerSize = 10
    'Selection.Marker.ForeColor.RGB = RGB(118, 113, 113)
        With Selection.Format.Line
        .Visible = msoFalse
        .ForeColor.ObjectThemeColor = msoThemeColorBackground2
        .ForeColor.TintAndShade = 0
        .ForeColor.RGB = RGB(81, 77, 117)
        .Transparency = 0
    End With
    With Selection.Format.Fill
       .Visible = msoTrue
       .ForeColor.ObjectThemeColor = msoThemeColorBackground2
       .ForeColor.TintAndShade = 0
       .ForeColor.RGB = RGB(81, 77, 117)
 '       .Transparency = 0
'        .Solid
   End With
    ActiveChart.FullSeriesCollection(3).Select
      Selection.Format.Line.Visible = msoFalse
     Selection.MarkerStyle = -4115
        Selection.MarkerSize = 10
       ' Selection.MarkerBackgroundColor.RGB = RGB(118, 113, 113)
        With Selection.Format.Line
        .Visible = msoFalse
        .ForeColor.ObjectThemeColor = msoThemeColorBackground2
        .ForeColor.TintAndShade = 0
        .ForeColor.RGB = RGB(81, 77, 117)
        .Transparency = 0
    End With
        With Selection.Format.Fill
       .Visible = msoTrue
        .ForeColor.ObjectThemeColor = msoThemeColorBackground2
       .ForeColor.TintAndShade = 0
        '.ForeColor.Brightness = -0.5 replaced with RBG
        .ForeColor.RGB = RGB(81, 77, 117)
'        .Transparency = 0
'        .Solid
    End With
  

  Next cht


'Next sht

CurrentSheet.Activate
Application.EnableEvents = True

End Sub

