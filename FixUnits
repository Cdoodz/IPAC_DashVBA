Sub Old_New_Unit_Consolidate()
'
' CLAB_Consolidate Macro
'

'CHANGE 17CP-OLD TO 17_CP IN ALL TABLES
'---TABLE CLAB_RATE
    Application.Goto Reference:="CLAB_Rate"
    Selection.Replace What:="17CP-OLD", Replacement:="17_CP", LookAt:=xlPart _
        , SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
'---TABLE CLAB_SIR
    Application.Goto Reference:="CLAB_SIR"
    Selection.Replace What:="17CP-OLD", Replacement:="17_CP", LookAt:=xlPart _
        , SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
'---TABLE CLABSI_CMS
    Application.Goto Reference:="CLABSI_CMS"
    Selection.Replace What:="17CP-OLD", Replacement:="17_CP", LookAt:=xlPart _
        , SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
